{
    "version": 2,
    "dists": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        },
        {
            "src": "/",
            "dest": "dist/server.js"
        },
        {
            "src": "/api/v1/(.*)",
            "dest": "dist/server.js"
        }
    ]
}